#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/workespace/next_prac/back/node_modules/.pnpm/typeorm@0.3.27_mysql2@3.15._eda8f88db52006a9e1a9b7657e07225d/node_modules/typeorm/node_modules:/mnt/c/workespace/next_prac/back/node_modules/.pnpm/typeorm@0.3.27_mysql2@3.15._eda8f88db52006a9e1a9b7657e07225d/node_modules:/mnt/c/workespace/next_prac/back/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/workespace/next_prac/back/node_modules/.pnpm/typeorm@0.3.27_mysql2@3.15._eda8f88db52006a9e1a9b7657e07225d/node_modules/typeorm/node_modules:/mnt/c/workespace/next_prac/back/node_modules/.pnpm/typeorm@0.3.27_mysql2@3.15._eda8f88db52006a9e1a9b7657e07225d/node_modules:/mnt/c/workespace/next_prac/back/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../.pnpm/typeorm@0.3.27_mysql2@3.15._eda8f88db52006a9e1a9b7657e07225d/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
else
  exec node  "$basedir/../.pnpm/typeorm@0.3.27_mysql2@3.15._eda8f88db52006a9e1a9b7657e07225d/node_modules/typeorm/cli-ts-node-commonjs.js" "$@"
fi
